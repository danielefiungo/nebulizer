#!/usr/bin/env node

/**
 * Module dependencies.
 */
require('dotenv-safe').config();

const debug = require('debug')('nebula:boot');
const { installNebula } = require('@fiungos/nebula');
// getting-started.js

(async function boot() {
  // const {} = process.env;
  try {
    // await something
    await installNebula();
  } catch (err) {
    debug('ðŸ’¥ ops', err);
    throw err;
  }
  // eslint-disable-next-line global-require
  require('../src/index');
})();
